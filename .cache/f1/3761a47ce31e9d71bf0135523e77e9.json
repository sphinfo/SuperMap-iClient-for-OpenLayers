ated":{"line":86,"column":42},"source":"src/mapRasterInformationQuery.js","original":{"line":70,"column":32}},{"generated":{"line":86,"column":44},"source":"src/mapRasterInformationQuery.js","original":{"line":70,"column":52},"name":"evt"},{"generated":{"line":86,"column":47},"source":"src/mapRasterInformationQuery.js","original":{"line":70,"column":55}},{"generated":{"line":86,"column":48},"source":"src/mapRasterInformationQuery.js","original":{"line":70,"column":56},"name":"coordinate"},{"generated":{"line":86,"column":58},"source":"src/mapRasterInformationQuery.js","original":{"line":70,"column":52}},{"generated":{"line":86,"column":59},"source":"src/mapRasterInformationQuery.js","original":{"line":70,"column":67}},{"generated":{"line":86,"column":60},"source":"src/mapRasterInformationQuery.js","original":{"line":70,"column":52}},{"generated":{"line":86,"column":61},"source":"src/mapRasterInformationQuery.js","original":{"line":70,"column":32}},{"generated":{"line":86,"column":62},"source":"src/mapRasterInformationQuery.js","original":{"line":70,"column":12}},{"generated":{"line":87,"column":2},"source":"src/mapRasterInformationQuery.js","original":{"line":72,"column":9}},{"generated":{"line":87,"column":3},"source":"src/mapRasterInformationQuery.js","original":{"line":59,"column":8}},{"generated":{"line":88,"column":0},"source":"src/mapRasterInformationQuery.js","original":{"line":73,"column":1}},{"generated":{"line":88,"column":1},"source":"src/mapRasterInformationQuery.js","original":{"line":43,"column":0}}],"sources":{"src/mapRasterInformationQuery.js":"import 'ol/ol.css';\r\nimport Map from 'ol/Map';\r\nimport View from 'ol/View';\r\nimport TileLayer from 'ol/layer/Tile';\r\nimport { TileSuperMapRest, GetGridCellInfosParameters, GridCellInfosService, } from '@supermap/iclient-ol';\r\nimport { Overlay } from 'ol';\r\n\r\n//지도 위에 표시할 오버레이 설정\r\nconst container = document.getElementById('popup');\r\nconst content = document.getElementById('popup-content');\r\nconst overlay = new Overlay({\r\n    element: container, //오버레이 요소\r\n    autoPan: true, //오버레이 표시 후 지도 이동 여부\r\n    autoPanAnimation :{\r\n        duration: 250 //애니메이션 지속 시간(밀리초)\r\n    }\r\n});\r\n\r\nconst baseUrl = \"https://iserver.supermap.io/iserver/services/map-world/rest/maps/世界地图_Day\";\r\nconst dataUrl = \"https://iserver.supermap.io/iserver/services/data-world/rest/data\";\r\n\r\nconst map = new Map({\r\n  target: 'map', //지도 컨테이너 ID\r\n  view: new View({\r\n      center: [0,0], //지도의 중심점\r\n      zoom: 4, //지도의 초기 해상도를 계산하는 확대/축소 레벨\r\n      projection: 'EPSG:4326' //지도 좌표계\r\n  }),\r\n  overlays: [overlay] //지도 오버레이추가\r\n});\r\n\r\nconst layer = new TileLayer({\r\n      source: new TileSuperMapRest({\r\n          url: baseUrl, //SuperMap iServer에 발행된 맵서비스 주소\r\n          wrapX: true //타일을 반경선 너머로 렌더링\r\n      }),\r\n      projection: 'EPSG:4326' //레이어 좌표계\r\n});\r\n\r\nmap.addLayer(layer);\r\n\r\n//map클릭 시 해당 위치의 픽셀 값 정보 표시\r\nmap.on(\"click\", function(evt){\r\n\r\n        var x = evt.coordinate[0];\r\n        var y = evt.coordinate[1];\r\n\r\n        if (x < -180.0 || x > 180.0 || y < -90 || y > 90) {\r\n            return;\r\n        }\r\n\r\n        var getGridCellInfosParam = new GetGridCellInfosParameters({\r\n            dataSourceName: \"World\",\r\n            datasetName: \"WorldEarth\",\r\n            X: x,\r\n            Y: y\r\n        });\r\n\r\n        new GridCellInfosService(dataUrl).getGridCellInfos(getGridCellInfosParam, function(serviceResult){\r\n\r\n            if (!serviceResult.result) {\r\n                return;\r\n            }\r\n\r\n            var result = serviceResult.result;\r\n            var innerHTML = \"Raster query results\" + \"<br>\" + \"column: \" + result.column + \"<br>\";\r\n            innerHTML += \"row: \" + result.row + \"<br>\";\r\n            innerHTML += \"value: \" + result.value + \"<br>\";\r\n            content.innerHTML = innerHTML;\r\n            overlay.setPosition([evt.coordinate[0], evt.coordinate[1]]);\r\n\r\n        });\r\n});"},"lineCount":null}},"error":null,"hash":"38ae12fa24c8ed259a080b536af3930b","cacheData":{"env":{}}}